  },
  "files": [
    "out"
  ],
  "keywords": [
    "autocomplete",
    "intellisense",
    "language",
    "prisma",
    "server",
    "formatting",
    "linting"
  ],
  "bugs": {
    "url": "https://github.com/prisma/vscode/issues"
  },
  "publishConfig": {
