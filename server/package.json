    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.3",
    "lint-staged": "10.1.2",
    "pkg-dir": "4.2.0",
    "prettier": "2.0.5",
    "prismafile": "1.0.3",
